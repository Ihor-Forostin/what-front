pipeline {
  agent any
  stages {
    stage('Build') {
      steps {
        sh 'npm config ls'
        sh 'npm install -g node-gyp'
        sh 'npm config set msvs_version 2017'
        sh 'npm install'
        sh 'npm audit fix --force'
        sh 'npm run build'
        emailext body: 'Test Message',
        subject: 'Test Subject',
        to: 'xiaomibestuser@gmail.com'
      }
    }
  }


}
